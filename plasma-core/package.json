{
  "name": "backend",
  "version": "0.0.1",
  "description": "backend",
  "main": "index.js",
  "bin": {
    "plasma": "/usr/src/app/dist/cli/bin/create-project"
  },
  "private": true,
  "scripts": {
    "development": "npm link & nodemon --watch src --exec babel-node src/index.js",
    "start": "node dist/index.js",
    "build": "babel src -d dist --ignore src/api/plugins/swagger-ui/",
    "demo": "babel-node src/demo",
    "demo:deposit": "babel-node src/demo/sendDeposit.js",
    "test-token-create-deposit": "mocha --require @babel/register --timeout 300000 --reporter spec ./test/token-create-deposit.js",
    "test-token-change-owner": "mocha --require @babel/register --timeout 300000 --reporter spec ./test/token-change-owner.js",
    "test-token-exit": "mocha --require @babel/register --timeout 300000 --reporter spec ./test/token-exit.js",
    "test-voting-for-candidate": "mocha --require @babel/register --timeout 900000 --reporter spec ./test/voting-for-candidate.js",
    "test-system": "mocha --require @babel/register --timeout 1800000 --reporter spec ./test/run-all.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "@hapi/boom": "^7.4.2",
    "@hapi/hapi": "^18.3.1",
    "@hapi/inert": "^5.2.1",
    "@hapi/joi": "^15.1.0",
    "@hapi/vision": "^5.5.2",
    "binary-data": "^0.6.0",
    "bn.js": "^5.0.0",
    "chai-things": "^0.2.0",
    "commander": "^2.20.0",
    "ethereumjs-util": "^6.1.0",
    "handlebars": "^4.2.0",
    "hapi-swagger": "^10.0.2",
    "ioredis": "^4.14.0",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "mongodb": "^3.3.3",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "rlp": "^2.2.3",
    "web3": "^1.2.1",
    "winston": "^2.3.1",
    "winston-daily-rotate-file": "^1.4.3",
    "ws": "^7.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/register": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-plugin-inline-json-import": "^0.3.2",
    "chai": "^4.1.2",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.6.1",
    "eslint-config-google": "^0.10.0",
    "mocha": "^5.1.1",
    "nodemon": "^1.19.1",
    "supertest": "^3.1.0"
  },
  "engines": {
    "node": ">=10.0",
    "npm": ">=3.0"
  },
  "license": "Proprietary"
}
