    "express": "^4.16.1",
    "gm": "^1.23.0",
    "lodash": "^4.17.4",
    "merkle-tools": "1.4.0",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "npm": "^6.1.0",
    "redis": "^2.8.0",
